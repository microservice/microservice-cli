{
  "name": "@microservices/ui",
  "version": "0.16.2",
  "scripts": {
    "build": "yarn build-app; ./scripts/build-workers.sh",
    "build-app": "rm -rf lib/*; parcel build src/index.html --out-dir lib --no-source-maps",
    "watch": "parcel watch src/index.html --out-dir lib --no-hmr"
  },
  "devDependencies": {
    "@microservices/validate": "^0.16.2",
    "@vue/component-compiler-utils": "3.0.2",
    "autoprefixer": "9.7.1",
    "core-js": "3.4.0",
    "eslint": "6.6.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "event-kit": "2.5.3",
    "idb-keyval": "3.2.0",
    "less": "3.10.3",
    "lodash": "4.17.15",
    "monaco-editor": "0.18.1",
    "normalize.css": "8.0.1",
    "parcel-bundler": "1.12.3",
    "promise.defer": "1.0.1",
    "sass": "1.23.3",
    "vue": "2.6.10",
    "vue-select": "3.2.0",
    "vue-template-compiler": "2.6.10",
    "vue-timeago": "5.1.2",
    "vue-toasted": "1.1.27",
    "vuex": "3.1.1",
    "yaml-ast-parser": "0.0.43"
  },
  "posthtml": {
    "recognizeSelfClosing": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/microservices/oms.git",
    "directory": "packages/oms-ui"
  },
  "main": "./index.js",
  "browserslist": [
    "last 5 chrome versions",
    "last 5 firefox versions",
    "last 5 safari versions"
  ],
  "description": "UI used in the cli with the command 'oms ui'",
  "bugs": {
    "url": "https://github.com/microservices/oms/issues"
  },
  "pkg": {
    "assets": "lib/*"
  },
  "homepage": "https://github.com/microservices/oms#readme",
  "license": "MIT"
}
